{
  "name": "eqcol",
  "version": "1.2.2",
  "description": "Equal effing columns",
  "license": "MIT",
  "repository": "awcross/eqcol",
  "author": {
    "name": "Alex Cross",
    "url": "alexcross.io"
  },
  "main": "dist/eqcol.js",
  "keywords": [
    "equal",
    "columns",
    "row",
    "height",
    "browser",
    "bootstrap",
    "foundation",
    "match"
  ],
  "scripts": {
    "build": "npm test && npm run bundle && npm run minify",
    "bundle": "rollup -c",
    "minify": "node_modules/.bin/uglifyjs dist/eqcol.js > dist/eqcol.min.js -m",
    "test": "xo"
  },
  "dependencies": {
    "customevent": "^1.0.0",
    "elem-dataset": "^1.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-env": "^1.1.1",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^5.0.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "uglify-js": "^2.7.3",
    "xo": "*"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "> 1%"
            ]
          },
          "modules": false,
          "loose": true
        }
      ]
    ],
    "plugins": [
      "transform-object-assign"
    ]
  },
  "xo": {
    "esnext": true,
    "envs": [
      "browser",
      "es6"
    ],
    "rules": {
      "padded-blocks": 0
    }
  }
}
